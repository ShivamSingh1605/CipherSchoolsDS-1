package Trees;
class Node{
	int data;
	Node left;
	Node right;
	Node(int data)
	{
		this.data=data;
		this.left=null;
		this.right=null;
	}
}
public class BinaryTree {
	Node root; //value will be updated here itself.
	BinaryTree()
	{
		root=null;
	}
	BinaryTree(int data)
	{
		this.root=new Node(data);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	}

}
