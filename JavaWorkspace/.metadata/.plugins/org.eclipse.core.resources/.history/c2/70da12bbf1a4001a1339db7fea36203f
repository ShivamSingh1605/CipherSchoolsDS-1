package stacks;
class MyStack{
	final int size=10;
	int arr[]=new int[size];
	int top=-1;
	boolean isEmpty()
	{
		if(top==-1) return true;
		return false;
	}
	int peek()
	{
		return arr[top];
	}
	void push(int data)
	{
//		top=top+1;
//		arr[top]=data;
		arr[++top]=data;
	}
	int pop()
	{
		top--;
		return arr[top];
	}
}
public class ArrayImp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
